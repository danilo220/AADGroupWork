                android:paddingRight="10dip"
                android:src="@drawable/road_sign_three" />

            <ImageView
                android:id="@+id/roadSignFive"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:paddingRight="10dip"
                android:src="@drawable/road_sign_five" />

            <ImageView
                android:id="@+id/roadSignTen"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:paddingRight="10dip"
                android:src="@drawable/road_sign_ten" />

            <ImageView
                android:id="@+id/roadSignSeven"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:paddingRight="10dip"
                android:src="@drawable/road_sign_seven" />
        </TableRow>

        <TableRow android:gravity="center">

            <ImageView
                android:id="@+id/roadSignTwo"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:paddingRight="10dip"
                android:src="@drawable/road_sign_two" />

            <ImageView
                android:id="@+id/roadSignEight"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:paddingRight="10dip"
                android:src="@drawable/road_sign_eight" />

            <ImageView
                android:id="@+id/roadSignTwelve"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:paddingRight="10dip"
                android:src="@drawable/road_sign_twelve" />

            <ImageView
                android:id="@+id/roadSignFour"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:paddingRight="10dip"
                android:src="@drawable/road_sign_four" />

            <ImageView
                android:id="@+id/roadSignSix"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:paddingRight="10dip"
                android:src="@drawable/road_sign_six" />

            <ImageView
                android:id="@+id/roadSignNine"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:paddingRight="10dip"
                android:src="@drawable/road_sign_nine" />
        </TableRow>
    </TableLayout>

    <Button
        android:id="@+id/UndoButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="85dp"
        android:layout_marginTop="13dp"
        android:text="Undo"
        app:layout_constraintStart_toStartOf="@+id/tableLayout3"
        app:layout_constraintTop_toBottomOf="@+id/tableLayout3" />

    <Button
        android:id="@+id/resetButtonRoadSign"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="83dp"
        android:layout_marginTop="13dp"
        android:text="Reset"
        app:layout_constraintEnd_toEndOf="@+id/tableLayout3"
        app:layout_constraintTop_toBottomOf="@+id/tableLayout3" />


</android.support.constraint.ConstraintLayout>

