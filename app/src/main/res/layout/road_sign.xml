<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/tableLayout2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="139dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TableRow android:gravity="center">

            <ImageView
                android:id="@+id/firstImage"
                android:layout_width="120dp"
                android:layout_height="145dp"
                android:paddingRight="10dip"
                android:src="@drawable/road_recognition_one" />

            <ImageView
                android:id="@+id/secondImage"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:paddingRight="10dip"
                android:src="@drawable/road_recognition_two" />

            <ImageView
                android:id="@+id/thirdImage"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:paddingRight="10dip"
                android:src="@drawable/road_recognition_three" />

            <ImageView
                android:id="@+id/fourthImage"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:paddingRight="10dip"
                android:src="@drawable/road_recognition_four" />
        </TableRow>

        <TableRow android:gravity="center">

            <ImageView
                android:id="@+id/fifthImage"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:paddingRight="10dip"
                android:src="@drawable/road_recognition_five" />

            <ImageView
                android:id="@+id/sixthImage"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:paddingRight="10dip"
                android:src="@drawable/road_recognition_six" />

            <ImageView
                android:id="@+id/seventhImage"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:paddingRight="10dip"
                android:src="@drawable/road_recognition_seven" />

            <ImageView
                android:id="@+id/eightImage"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:paddingRight="10dip"
                android:src="@drawable/road_recognition_eight" />
        </TableRow>

        <TableRow android:gravity="center">

            <ImageView
                android:id="@+id/nithImage"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:paddingRight="10dip"
                android:src="@drawable/road_recognition_nine" />

            <ImageView
                android:id="@+id/tenthImage"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:paddingRight="10dip"
                android:src="@drawable/road_recognition_ten" />

            <ImageView
                android:id="@+id/eleventhImage"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:paddingRight="10dip"
                android:src="@drawable/road_recognition_eleven" />

            <ImageView
                android:id="@+id/twelvethImage"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:paddingRight="10dip"
                android:src="@drawable/road_recognition_twelve" />
        </TableRow>
    </TableLayout>


    <TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/tableLayout3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableLayout2">

        <TableRow android:gravity="center">

            <ImageView
                android:id="@+id/roadSignOne"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:paddingRight="10dip"
                android:src="@drawable/road_sign_one" />

            <ImageView
                android:id="@+id/roadSignEleven"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:paddingRight="10dip"
                android:src="@drawable/road_sign_eleven" />

            <ImageView
                android:id="@+id/roadSignThree"
                android:layout_width="70dp"
